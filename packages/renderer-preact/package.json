{
  "name": "@framejs/renderer-preact",
  "version": "1.0.0-rc.4",
  "description": "A Framejs renderer for preact",
  "main": "dist/renderer-preact.js",
  "types": "dist/renderer-preact.d.ts",
  "scripts": {
    "build": "node ./node_modules/typescript/bin/tsc",
    "bundle": "node node_modules/rollup/bin/rollup dist/renderer-preact.js -f iife -g preact:h,preact:render -n rendererPreact -o dist/renderer-preact.js",
    "bundle:esm": "node node_modules/rollup/bin/rollup dist/renderer-preact.js -f es -o dist/renderer-preact.esm.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/framejs/framejs.git"
  },
  "keywords": [
    "components",
    "custom",
    "dom",
    "elements",
    "web",
    "typescript",
    "vdom"
  ],
  "author": "Emil MÃ¸ller <emilrmoeller@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/framejs/framejs/issues"
  },
  "homepage": "https://github.com/framejs/framejs#readme",
  "devDependencies": {
    "preact": "^8.2.7",
    "rollup": "^0.57.1",
    "typescript": "^2.6.2"
  }
}
